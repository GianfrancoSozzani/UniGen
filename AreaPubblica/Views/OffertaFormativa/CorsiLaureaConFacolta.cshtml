@model List<AreaPubblica.Models.Entities.Corso>

@{
	ViewData["Title"] = "Corsi con Facoltà";
	var dati = ViewBag.Dati as List<dynamic>;
}
<div style="height: 10vh;">&nbsp;</div>
<div class="container">




<h2 class="mt-5">Corsi di Laurea con Facoltà</h2>
<table class="table table-striped">
	<thead>
		<tr>
			<th>Titolo Corso</th>
			<th>Facoltà</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in dati)
		{
			<tr>
				<td>@item.TitoloCorso</td>
				<td>@item.Facolta</td>
			</tr>
		}
	</tbody>
</table>
</div>